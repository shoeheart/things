{
  "name": "things",
  "private": true,
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@rails/webpacker": "https://github.com/rails/webpacker.git",
    "auth0-lock": "^11.11.0",
    "aws-sdk": "^2.353.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "eslint-plugin-react": "^7.11.1",
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.14.5",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.4.0",
    "prop-types": "^15.6.2",
    "rails-ujs": "^5.2.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react_ujs": "^2.4.4",
    "reactstrap": "^6.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "config/webpack/**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ],
      "frontend/**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ],
      "frontend/**/*.css": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ]
    },
    "ignore": [
      "frontend/styles/reduction.css"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack-dev-server": "^3.1.10"
  }
}
