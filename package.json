    "lint-staged": "^7.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack-dev-server": "^3.1.9"
  }
